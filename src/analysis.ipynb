    "    \n",
    "    print(f\"📊 Réseau 1960-1970 : {len(G_early.nodes())} nœuds, {len(G_early.edges())} liens\")\n",
    "    visualize_cooccurrence_network(G_early, \"Réseau de Co-occurrence : 'WOMAN' (1960-1970)\", top_n=25)\n",
    "    \n",
    "    # Réseau pour les années 2000-2010\n",
    "    recent_df = df[df['decade'].isin([2000, 2010])]\n",
