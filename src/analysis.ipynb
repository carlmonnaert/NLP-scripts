    "# =====================================================\n",
    "# PHASE 3 : GRAPH MINING - Réseaux de Co-occurrence\n",
    "# Concept : Visualiser les \"bulles\" de mots associés\n",
    "# Question : Les associations se déplacent-elles vers des thèmes valorisants ?\n",
    "# =====================================================\n",
    "\n",
    "import networkx as nx\n",
    "from itertools import combinations\n",
    "\n",
    "def build_cooccurrence_graph(tokens_list, target_words, window=5, min_freq=5):\n",
    "    \"\"\"\n",
    "    Construit un graphe de co-occurrence.\n",
    "    \n",
    "    Nœuds : Mots\n",
    "    Liens : Co-occurrence (poids = fréquence)\n",
    "    \"\"\"\n",
    "    G = nx.Graph()\n",
    "    edge_weights = defaultdict(int)\n",
    "    target_set = set(w.lower() for w in target_words)\n",
    "    \n",
    "    for tokens in tokens_list:\n",
    "        for i, token in enumerate(tokens):\n",
    "            if token.lower() in target_set:\n",
    "                # Extraire le contexte\n",
    "                start = max(0, i - window)\n",
    "                end = min(len(tokens), i + window + 1)\n",
    "                context = tokens[start:i] + tokens[i+1:end]\n",
    "                \n",
    "                # Créer des liens entre le mot cible et son contexte\n",
    "                for context_word in context:\n",
    "                    if context_word != token:\n",
    "                        edge = tuple(sorted([token, context_word]))\n",
    "                        edge_weights[edge] += 1\n",
    "    \n",
    "    # Ajouter les arêtes au graphe (filtrer par fréquence minimale)\n",
    "    for (word1, word2), weight in edge_weights.items():\n",
    "        if weight >= min_freq:\n",
    "            G.add_edge(word1, word2, weight=weight)\n",
    "    \n",
    "    return G\n",
    "\n",
    "\n",
    "def visualize_cooccurrence_network(G, title, top_n=30):\n",
    "    \"\"\"\n",
    "    Visualise le réseau de co-occurrence avec NetworkX.\n",
    "    \"\"\"\n",
    "    if len(G.nodes()) == 0:\n",
    "        print(f\"⚠️  Graphe vide pour : {title}\")\n",
    "        return\n",
    "    \n",
    "    # Garder seulement les top_n nœuds les plus connectés\n",
    "    degrees = dict(G.degree())\n",
    "    top_nodes = sorted(degrees, key=degrees.get, reverse=True)[:top_n]\n",
    "    G_sub = G.subgraph(top_nodes)\n",
    "    \n",
    "    # Calculer le layout\n",
    "    pos = nx.spring_layout(G_sub, k=0.5, iterations=50)\n",
    "    \n",
    "    # Tailles des nœuds proportionnelles au degré\n",
    "    node_sizes = [degrees[node] * 100 for node in G_sub.nodes()]\n",
    "    \n",
    "    # Épaisseurs des arêtes proportionnelles au poids\n",
    "    edges = G_sub.edges()\n",
    "    weights = [G_sub[u][v]['weight'] for u, v in edges]\n",
    "    \n",
    "    # Dessin\n",
    "    plt.figure(figsize=(14, 10))\n",
    "    \n",
    "    nx.draw_networkx_nodes(G_sub, pos, node_size=node_sizes, \n",
    "                           node_color='lightblue', alpha=0.7)\n",
    "    nx.draw_networkx_edges(G_sub, pos, width=weights, alpha=0.3, edge_color='gray')\n",
    "    nx.draw_networkx_labels(G_sub, pos, font_size=10, font_weight='bold')\n",
    "    \n",
    "    plt.title(title, fontsize=16, fontweight='bold')\n",
    "    plt.axis('off')\n",
    "    plt.tight_layout()\n",
    "    plt.show()\n",
    "\n",
    "\n",
    "if not df.empty:\n",
    "    print(\"🕸️  Construction des réseaux de co-occurrence...\")\n",
    "    print(\"   Application : Graph Mining - visualiser l'évolution sémantique\\n\")\n",
    "    \n",
    "    # Réseau pour les années 1960-1970\n",
    "    early_df = df[df['decade'].isin([1960, 1970])]\n",
