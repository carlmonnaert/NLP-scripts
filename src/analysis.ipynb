    "# =====================================================\n",
    "# PHASE 7 : WORD EMBEDDINGS - Évolution Sémantique (OPTIONNEL)\n",
    "# Concept : Word2Vec Diachronique - Analyser les glissements de sens\n",
    "# Question : \"Woman\" est-il associé à différents concepts selon les décennies ?\n",
    "# =====================================================\n",
    "\n",
    "from gensim.models import Word2Vec\n",
    "\n",
    "def train_word2vec_by_decade(df, vector_size=100):\n",
    "    \"\"\"\n",
    "    Entraîne un modèle Word2Vec par décennie.\n",
    "    Application : Linguistique computationnelle - Sémantique distributionnelle\n",
    "    \"\"\"\n",
    "    models = {}\n",
    "    \n",
    "    for decade in sorted(df['decade'].unique()):\n",
    "        decade_tokens = df[df['decade'] == decade]['clean_tokens'].tolist()\n",
    "        \n",
    "        if len(decade_tokens) > 10:  # Assez de données\n",
    "            print(f\"   Entraînement Word2Vec pour {decade}s...\")\n",
    "            model = Word2Vec(\n",
    "                sentences=decade_tokens,\n",
    "                vector_size=vector_size,\n",
    "                window=5,\n",
    "                min_count=5,\n",
    "                workers=4,\n",
    "                sg=1  # Skip-gram\n",
    "            )\n",
    "            models[decade] = model\n",
    "    \n",
    "    return models\n",
    "\n",
    "\n",
    "def analyze_word_evolution(models, target_word, topn=10):\n",
    "    \"\"\"\n",
    "    Analyse l'évolution sémantique d'un mot à travers les décennies.\n",
    "    \"\"\"\n",
    "    results = []\n",
    "    \n",
    "    for decade, model in models.items():\n",
    "        if target_word in model.wv:\n",
    "            neighbors = model.wv.most_similar(target_word, topn=topn)\n",
    "            neighbors_words = [w for w, score in neighbors]\n",
    "            \n",
    "            results.append({\n",
    "                'decade': decade,\n",
    "                'top_associated_words': ', '.join(neighbors_words[:5]),\n",
    "                'num_neighbors': len(neighbors)\n",
    "            })\n",
    "    \n",
    "    return pd.DataFrame(results)\n",
    "\n",
    "\n",
    "if not df.empty:\n",
    "    print(\"🧠 Entraînement de Word2Vec par décennie...\")\n",
    "    print(\"   Concept : Sémantique distributionnelle (Maria Boritchev)\")\n",
    "    print(\"   'Les mots sont définis par leur contexte'\\n\")\n",
    "    \n",
    "    # Entraîner les modèles\n",
    "    w2v_models = train_word2vec_by_decade(df, vector_size=100)\n",
    "    \n",
    "    print(f\"\\n✅ {len(w2v_models)} modèles entraînés\")\n",
    "    \n",
    "    # Analyser l'évolution de \"woman\"\n",
    "    print(\"\\n📊 Évolution sémantique de 'WOMAN' :\")\n",
    "    df_woman_evolution = analyze_word_evolution(w2v_models, 'woman', topn=10)\n",
    "    display(df_woman_evolution)\n",
    "    \n",
    "    # Analyser l'évolution de \"black\"\n",
    "    print(\"\\n📊 Évolution sémantique de 'BLACK' :\")\n",
    "    df_black_evolution = analyze_word_evolution(w2v_models, 'black', topn=10)\n",
    "    display(df_black_evolution)\n",
    "    \n",
    "    print(\"\\n   👉 Interprétation :\")\n",
    "    print(\"      - Les mots associés révèlent les contextes dominants\")\n",
    "    print(\"      - Déplacement vers des thèmes valorisants = progrès\")\n",
    "    print(\"      - Persistance de stéréotypes = biais structurels\")\n",
    "    \n",
    "    # Sauvegarder les modèles\n",
    "    output_dir = Path('../results/models')\n",
    "    output_dir.mkdir(parents=True, exist_ok=True)\n",
